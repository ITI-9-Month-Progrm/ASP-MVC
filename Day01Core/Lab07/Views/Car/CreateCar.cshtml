
@{
    ViewBag.Title = "CreateCar";

}

<h2>Create New Car</h2>
<center>
    <fieldset class="border">
        <legend>Car Details</legend>
        <form action="/Car/CreateCar/" method="post">
            <label style="margin:5px">CarID</label>
            <input style="margin-left: 83px;" type="text" name="id" />
            <br />
            <label style="margin:5px">CarColor</label>
            <input style="margin-left: 63px;" type="text" name="color" />
            <br />
            <label style="margin:5px">CarModel</label>
            <input style="margin-left: 63px;" type="text" name="model" />
            <br />
            <label style="margin:5px">CarManfacture</label>
            <input style="margin-left: 32px; " type="text" name="manfactour" />
            <br />
            <br />
            <input type="submit" value="Save" style="margin-left:10%" class="btn btn-primary" />
        </form>
    </fieldset>
</center>
